function [z,X,Y] = q2_logdf(ranges, landmarks, sig, n_sig)    
    [X,Y] = meshgrid(linspace(-2,2,50));
    z = meshgrid(linspace(-2,2,50));
    
    for x = length(X)
        for y = length(Y)
            t = [X(x,y); Y(x,y)];
            
            z(x,y) = -((1/2)*t'*sig*eye(2)*t) ...
                - ((1/(2*n_sig))*sum((ranges - vecnorm(t - landmarks)).^2));
        end
    end
end